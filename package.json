{
  "name": "fantasy-gather-bot",
  "version": "1.0.0",
  "description": "Production-ready Telegram and Discord bot for NFL fantasy with Sleeper integration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "vercel-build": "prisma generate && next build",
    "legacy:start": "node dist/index.js",
    "legacy:dev": "tsx watch src/index.ts",
    "legacy:build": "tsc",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts app/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.ts app/**/*.{ts,tsx} --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "telegram",
    "discord",
    "bot",
    "nfl",
    "fantasy",
    "sleeper"
  ],
  "author": "Anton Kravchuk",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "discord.js": "^14.22.1",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "next": "^15.5.2",
    "node-cron": "^3.0.3",
    "pino": "^8.17.2",
    "prisma": "^5.7.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "supertest": "^7.1.4",
    "telegraf": "^4.15.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vercel/node": "^3.0.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vercel": "^32.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
